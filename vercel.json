{
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
    
      "version": 2,
      "builds": [
        {
          "src": "app.js",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "app.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "https://frontends-psi.vercel.app"
          }
        }
      ]
    }
  
  